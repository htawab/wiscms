<?xml version="1.0" encoding="utf-8" ?>
<Setting>
  <Theme>Office2003</Theme>
  <Toolbars>
    <Toolbar>
      <Name>Clipboard</Name>
      <ToolbarItems>
        <ToolbarItem>
          <Name>Paste</Name>
          <Type>ToolbarButton</Type>
          <Title>粘贴</Title>
          <Function>HtmlEditor_Paste</Function>
        </ToolbarItem>
        <ToolbarItem>
          <Name>Cut</Name>
          <Type>ToolbarButton</Type>
          <Title>剪切</Title>
          <Function>HtmlEditor_Cut</Function>
        </ToolbarItem>
        <ToolbarItem>
          <Name>Copy</Name>
          <Type>ToolbarButton</Type>
          <Title>复制</Title>
          <Function>HtmlEditor_Copy</Function>
        </ToolbarItem>
        <ToolbarItem>
          <Name>Clear</Name>
          <Type>ToolbarButton</Type>
          <Title>清空编辑器中所有的文字和 HTML 代码</Title>
          <Function>HtmlEditor_Clear</Function>
        </ToolbarItem>
        <ToolbarItem>
          <Name>Redo</Name>
          <Type>ToolbarButton</Type>
          <Title>重做</Title>
          <Function>HtmlEditor_Redo</Function>
        </ToolbarItem>
        <ToolbarItem>
          <Name>Undo</Name>
          <Type>ToolbarButton</Type>
          <Title>撤销</Title>
          <Function>HtmlEditor_Undo</Function>
        </ToolbarItem>
      </ToolbarItems>
    </Toolbar>
    <Toolbar>
      <Name>Font</Name>
      <ToolbarItems>
        <ToolbarItem>
          <Name>Styles</Name>
          <Type>ToolbarSelect</Type>
          <Title>段落格式</Title>
          <Function>HtmlEditor_Styles</Function>
        </ToolbarItem>
        <ToolbarItem>
          <Name>FontFaces</Name>
          <Type>ToolbarSelect</Type>
          <Title> 字体 </Title>
          <Function>HtmlEditor_FontFaces</Function>
          <ListItems>
          </ListItems>
        </ToolbarItem>
        <ToolbarItem>
          <Name>FontSizes</Name>
          <Type>ToolbarSelect</Type>
          <Title>字号</Title>
          <Function>HtmlEditor_FontSizes</Function>
        </ToolbarItem>
        <ToolbarItem>
          <Name>PreFormatted</Name>
          <Type>ToolbarButton</Type>
          <Title>预先格式化的内容</Title>
          <Function>HtmlEditor_PreFormatted</Function>
        </ToolbarItem>
        <ToolbarItem>
          <Name>Address</Name>
          <Type>ToolbarButton</Type>
          <Title>联系信息，比如地址和签名</Title>
          <Function>HtmlEditor_Address</Function>
        </ToolbarItem>
        <ToolbarItem>
          <Name>RemoveFormat</Name>
          <Type>ToolbarButton</Type>
          <Title>清除格式</Title>
          <Function>HtmlEditor_RemoveFormat</Function>
        </ToolbarItem>
        <ToolbarItem>
          <Name>StripWordFormatting</Name>
          <Type>ToolbarButton</Type>
          <Title>清除 Word 格式</Title>
          <Function>HtmlEditor_StripWordFormatting</Function>
        </ToolbarItem>
        <ToolbarItem>
          <Name>Bold</Name>
          <Type>ToolbarButton</Type>
          <Title>粗体</Title>
          <Function>HtmlEditor_Bold</Function>
        </ToolbarItem>
        <ToolbarItem>
          <Name>Italic</Name>
          <Type>ToolbarButton</Type>
          <Title>斜体</Title>
          <Function>HtmlEditor_Italic</Function>
        </ToolbarItem>
        <ToolbarItem>
          <Name>Underline</Name>
          <Type>ToolbarButton</Type>
          <Title>下划线</Title>
          <Function>HtmlEditor_Underline</Function>
        </ToolbarItem>
        <ToolbarItem>
          <Name>Strikethrough</Name>
          <Type>ToolbarButton</Type>
          <Title>删除线</Title>
          <Function>HtmlEditor_Strikethrough</Function>
        </ToolbarItem>
        <ToolbarItem>
          <Name>Subscript</Name>
          <Type>ToolbarButton</Type>
          <Title>下标</Title>
          <Function>HtmlEditor_Subscript</Function>
        </ToolbarItem>
        <ToolbarItem>
          <Name>Superscript</Name>
          <Type>ToolbarButton</Type>
          <Title>上标</Title>
          <Function>HtmlEditor_Superscript</Function>
        </ToolbarItem>
        <ToolbarItem>
          <Name>ChangeCase</Name>
          <Type>ToolbarButton</Type>
          <Title>更改大小写</Title>
          <Function>HtmlEditor_ChangeCase</Function>
        </ToolbarItem>
        <ToolbarItem>
          <Name>FontColors</Name>
          <Type>ToolbarSelect</Type>
          <Title>字体颜色</Title>
          <Function>HtmlEditor_FontColors</Function>
