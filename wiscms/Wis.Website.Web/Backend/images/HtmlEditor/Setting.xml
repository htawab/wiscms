        <ToolbarItem>
          <Name>JustifyRight</Name>
          <Type>ToolbarButton</Type>
          <Title>右对齐</Title>
          <Function>HtmlEditor_JustifyRight</Function>
        </ToolbarItem>
        <ToolbarItem>
          <Name>JustifyFull</Name>
          <Type>ToolbarButton</Type>
          <Title>两端对齐</Title>
          <Function>HtmlEditor_JustifyFull</Function>
        </ToolbarItem>
